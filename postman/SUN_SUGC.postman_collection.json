  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "packages": {},
        "requests": {},
        "exec": [
          "// console.log('iniciando');\r",
          "const token = pm.globals.get('JWT_TKN');\r",
          "const loginUrl = `${pm.variables.get('protocol')}://${pm.variables.get('host')}${pm.variables.get('port')}/api/v1/login`;\r",
          "// console.log('globals.JWT_TKN', token);\r",
          "if(!token){\r",
          "    pm.sendRequest({\r",
          "        url: loginUrl,\r",
          "        method: 'POST',\r",
          "        header: { 'content-type': 'application/json' },\r",
          "        body: {\r",
          "            mode: 'raw',\r",
          "            raw: JSON.stringify({\r",
          "                correo: pm.variables.get('user'),\r",
          "                contrasena: pm.variables.get('pass')\r",
          "            })\r",
          "        }\r",
          "    }, (err, response) => {\r",
          "        const jsonResponse = response.json();\r",
          "        // console.log(jsonResponse);\r",
          "        const { data: {token}} = jsonResponse;\r",
          "        // console.log('token', token)\r",
          "        pm.globals.set('JWT_TKN', token);\r",
          "    });\r",
          "}"
        ]
      }
    },
